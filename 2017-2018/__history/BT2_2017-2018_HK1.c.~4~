#INCLUDE<16F887.H>
#DEVICE ADC=10
#FUSES HS
#USE DELAY(CLOCK=11.0592M)

#DEFINE LD1 PIN_C1
#DEFINE LD2 PIN_C2

VOID READ_AN5()
{
   SET_ADC_CHANNEL(5)
}

CONST UNSIGNED INT8 MA7DOAN[10] = {0XC0, 0XF9, 0XA4, 0XB0, 0X99, 0X92, 0X82, 0XF8, 0X80, 0X90};
UNSIGNED INT8 I;
UNSIGNED INT16 TEMP1,TEMP2,OLDTEMP1,OLDTEMP2;

VOID MAIN()
{
   SET_TRIS_C(0B11000001);
   SET_TRIS_D(0X00);
   SET_TRIS_B(0X00);
   //SETUP ADC
   SETUP_ADC(ADC_CLOCK_INTERNAL);
   SETUP_ADC_PORTS(SAN5|SAN6|VSS_VDD);
}
